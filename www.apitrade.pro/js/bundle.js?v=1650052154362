            .attr('data-offset','0, 20px')
            .attr('data-placement','top')
            .attr('title','<h4 class="mb-2">'+chartLabels[key]+' your portfolio</h4><h5>'+value+' BTC</h5>')
            .tooltip('update')
    })

    setTimeout( function() {
        var points = jQuery('.ct-point')
        var newSelectedPoint = jQuery(points.get(6))
        //newSelectedPoint.addClass('ct-point-active').tooltip('show')
    }, 1000)

